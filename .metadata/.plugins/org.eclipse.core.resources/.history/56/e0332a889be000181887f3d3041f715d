#include <bits/stdc++.h>
using namespace std;

bool diffByOne(string a, string b) {
	if (a.size() != b.size()) return false;
	int count = 0;
	for (int i = 0; i< a.size(); i++) {
		if (a[i] != b[i]) {
			count ++;
		}
	}
	return count == 1;
}

