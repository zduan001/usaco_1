#include <bits/stdc++.h>
using namespace std;

int main() {
	int n;
	float rate;
	while(cin >> n) {
		vector<vector<float>> w (n, vector<float>(0));
		for (int i = 0; i < n; i ++) {
			for (int j = 0; j < n-1; j ++) {
				if (i == j) {
					w[i].push_back(1.0f);
					continue;
				}
				cin >> rate;
				w[i].push_back(rate);
			}
		}

		vector<vector<int>> p(n, vector<int>(n, 0));
		for (int i = 0; i < n; i++) {
			for (int j = 0; j < n; j++) {
				p[i][j]= i;
			}
		}

		for (int k = 0; k < n; k++) {
			for (int i = 0; i < n; i++) {
				for (int j = 0; j < n; j++) {
					if (i == j) continue;
					if (w[i][k] * w[[k][j] > w[i][j]) {
						w[i][j] = w[i][k] * w[k][j];
						p[i][j] = k;
					}
				}
			}
		}

		for (int i = 0; i < n; i++) {
			for (int j = 0; j < n; j++) {
				if (w[i][j] * w[j][i] > 1) {

				}
			}
		}
	}

	return 0;
}
