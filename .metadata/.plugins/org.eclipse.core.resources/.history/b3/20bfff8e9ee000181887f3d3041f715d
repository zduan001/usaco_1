#include <bits/stdc++.h>
using namespace std;

bool diffByOne(string a, string b) {
	if (a.size() != b.size()) return false;
	int count = 0;
	for (int i = 0; i< (int)a.size(); i++) {
		if (a[i] != b[i]) {
			count ++;
		}
	}
	return count == 1;
}

int findDistance(
		unordered_map<string, vector<string>> graph,
		string s,
		string e) {

	queue<string> q;
	q.push(s);
	q.push("");
	int length = 0;
	unordered_set<string> visited;
	while(!q.empty()) {
		string tmp = q.front();
		visited.insert(tmp);
		q.pop();
		if (tmp == e) {break;}
		if (tmp == "") {
			if (q.empty()) {return -1;}
			q.push("");
			length++;
		}
		for(auto s : graph[tmp]) {
			if (visited.find(s) != visited.end()) {continue;}
			q.push(s);
		}
	}
	return length;
}

int main() {
	int n;
	cin >> n;
	for (int i = 0; i < n; i++) {
		vector<string> words;
		string tmp;
		string stop = "*";
		while(cin >> tmp) {
			if (tmp == stop) break;
			words.push_back(tmp);
		}

		unordered_map<string, vector<string>> graph;
		for(int j = 0; j < (int)words.size(); j++) {
			for (int k = 0; k < (int)words.size(); k++) {
				if (diffByOne(words[i], words[j])) {
					graph[words[i]].push_back(words[j]);
					graph[words[j]].push_back(words[i]);
				}
			}
		}

		string start, end;
		string line;
		while(1)
		{
			getline(cin, line);
			if (line.size() == 0) break;
			istringstream iss(line);
			iss >> start >> end;
			cout << findDistance(graph, start, end);
		}
	}

	return 0;
}
