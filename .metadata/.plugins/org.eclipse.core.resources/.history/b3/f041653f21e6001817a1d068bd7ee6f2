#include <bits/stdc++.h>
using namespace std;

int main() {
	int length;
	while(1) {
		cin >> length;
		if (!length) {return 0;}
		int n, tmp;
		cin >> n;
		vector<int> cuts;
		cuts.push_back(0);
		for(int i = 0; i < n; i++) {
			cin >> tmp;
			cuts.push_back(tmp);
		}
		cuts.push_back(length);

	}


	return 0;
}
