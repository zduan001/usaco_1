#include <bits/stdc++.h>
using namespace std;

int main() {
	int n, m;
	cin >> n >>m;
	vector<vector<int>> graph(n, vector<int>(n, 0));
	int u, v;
	char t;
	for (int i = 0; i <  m; i++) {
		cin >> u >> v >> t;
		graph[u-1][v-1] = t =='T' ? 1 : -1;
	}

	for (int i = 0; i < n; i++) {
		for (int j = 0; j < n; j ++) {
			cout << graph[i][j] << " ";
		}
		cout << endl;
	}
	return 0;
}
